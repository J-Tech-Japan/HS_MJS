<?xml version="1.0" encoding="utf-8"?>
<configuration>
<configSections>
	<sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
		<section name="WordAddIn1.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
	</sectionGroup>
</configSections>

<!-- Trace設定を改善 -->
<system.diagnostics>
	<trace autoflush="true">
		<listeners>
			<!-- デフォルトリスナーを削除してパフォーマンス向上 -->
			<remove name="Default" />

			<add name="textFileListener"
				 type="System.Diagnostics.TextWriterTraceListener"
				 initializeData="C:\temp\logMarker.txt"
				 traceOutputOptions="DateTime,ProcessId,ThreadId" />
		</listeners>
	</trace>
</system.diagnostics>

<userSettings>
	<WordAddIn1.Properties.Settings>
		<setting name="user_id" serializeAs="String">
			<value>0</value>
		</setting>
		<setting name="tbFilePathLast" serializeAs="String">
			<value />
		</setting>
		<setting name="tbDisplayStrLast" serializeAs="String">
			<value />
		</setting>
		<setting name="tbURLLast" serializeAs="String">
			<value />
		</setting>
		<setting name="extractHighQualityImages" serializeAs="String">
			<value>True</value>
		</setting>
		<setting name="isBetaMode" serializeAs="String">
			<value>False</value>
		</setting>
		<setting name="outputScaleMultiplier" serializeAs="String">
			<value>1.4</value>
		</setting>
		<setting name="tableImageScaleMultiplier" serializeAs="String">
			<value>1.2</value>
		</setting>
		<setting name="columnImageScaleMultiplier" serializeAs="String">
			<value>1.2</value>
		</setting>
	</WordAddIn1.Properties.Settings>
</userSettings>
<startup>
	<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
</startup>
<runtime>
	<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
		<dependentAssembly>
			<assemblyIdentity name="DocumentFormat.OpenXml" publicKeyToken="8fb06cb64d019a17" culture="neutral" />
			<bindingRedirect oldVersion="0.0.0.0-3.3.0.0" newVersion="3.3.0.0" />
		</dependentAssembly>
	</assemblyBinding>
</runtime>
</configuration>
